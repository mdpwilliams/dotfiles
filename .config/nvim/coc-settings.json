{
  "diagnostic.enable": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": "> ",
  "diagnostic.messageTarget": "echo",
  "diagnostic.messageDelay": 100,
  "diagnostic.format": "[%source%code] %message",
  "codeLens.enable": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "react",
    "typescriptreact",
    "typescript"
  ],
  "coc.source.around.priority": 80,
  "coc.source.buffer.priority": 90,
  "snippets.priority": 100,
  "snippets.extends": {
    "typescriptreact": ["react", "javascript"]
  },
  "yank.priority": 20
}
